{
	"settings": 
	{
		"number_of_shards": 5,
		"analysis": 
		{
			"analyzer": 
			{
				"full_name": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"syn_indexer": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"sgx_synonyms"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"starts_with": 
				{
					"filter": 
					[
						"lowercase"
					],

					"tokenizer": "keyword"
				},

				"partial_ticker": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"ticker_edgeNngram_tokenizer"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"partial_ticker_middle": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"ticker_ngram_tokenizer"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"partial_ticker_back": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"reverse",
						"ticker_edgeNngram_tokenizer",
						"reverse"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"partial_name": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"name_edgeNgram_tokenizer"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"partial_name_middle": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"name_ngram_tokenizer"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"partial_name_back": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"reverse",
						"name_edgeNgram_tokenizer",
						"reverse"
					],

					"type": "custom",
					"tokenizer": "standard"
				}
			},

			"filter": 
			{
				"name_edgeNgram_tokenizer": 
				{
					"type": "edgeNgram",
					"min_gram": "2",
					"max_gram": "20"
				},

				"name_ngram_tokenizer": 
				{
					"type": "ngram",
					"min_gram": "3",
					"max_gram": "20"
				},

				"ticker_edgeNngram_tokenizer": 
				{
					"type": "edgeNgram",
					"min_gram": "1",
					"max_gram": "5"
				},

				"ticker_ngram_tokenizer": 
				{
					"type": "ngram",
					"min_gram": "1",
					"max_gram": "5"
				},

				"sgx_synonyms": 
				{
					"type": "synonym",
					"synonyms": 
					[
						"reit, real estate inv trust, real estate investment trust, reits",
						"ltd, limited",
						"hldg, hldgs, holding, holdings",
						"corp, cor, corporation",
						"mfg, manufacturing",
						"grp, group",
						"co, company",
						"intl, int'l, international"
					]
				}
			}
		}
	},


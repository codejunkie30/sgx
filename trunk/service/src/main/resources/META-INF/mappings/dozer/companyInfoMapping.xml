<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

  <configuration>
    <stop-on-errors>true</stop-on-errors>
    <date-format>MM/dd/yyyy HH:mm</date-format>
    <wildcard>true</wildcard>
    <custom-converters>
	<converter type="com.wmsi.sgx.conversion.dozer.StringToDoubleConverter">
		<class-a>java.lang.String</class-a>
		<class-b>java.lang.Double</class-b>
	</converter>
	</custom-converters>    
  </configuration>
  
  <mapping wildcard="false">
    <class-a>java.util.Map</class-a>
    <class-b>com.wmsi.sgx.model.CompanyInfo</class-b>
      <field>
        <a key="IQ_COMPANY_NAME">this</a>
        <b>companyName</b>
      </field>
      <field>
      	<a key="IQ_COMPANY_TICKER_NO_EXCH">this</a>
        <b>tickerCode</b>
      </field>
      <field>
        <a key="IQ_OPENPRICE">this</a>
        <b>openPrice</b>
      </field>
      <field>
        <a key="IQ_CLOSEPRICE">this</a>
        <b>closePrice</b>
      </field>      
      <field>
        <a key="IQ_CLOSEPRICE_prev">this</a>
        <b>previousClosePrice</b>
      </field>
      
      <field>
        <a key="IQ_YEARHIGH">this</a>
        <b>yearHigh</b>
      </field>
      <field>
        <a key="IQ_YEARLOW">this</a>
        <b>yearLow</b>
      </field>
      <field>
        <a key="IQ_VOLUME">this</a>
        <b>volume</b>
      </field>
      <field>
        <a key="IQ_MARKETCAP">this</a>
        <b>marketCap</b>
      </field>
      <field>
        <a key="IQ_TOTAL_REV">this</a>
        <b>totalRevenue</b>
      </field>
      <field>
        <a key="IQ_INDUSTRY">this</a>
        <b>industry</b>
      </field>
      <field>
        <a key="IQ_INDUSTRY_GROUP">this</a>
        <b>industryGroup</b>
      </field>
      <field>
        <a key="IQ_DIVIDEND_YIELD">this</a>
        <b>dividendYield</b>
      </field>
      <field>
        <a key="IQ_PE_EXCL_FWD">this</a>
        <b>peRatio</b>
      </field>
      <field>
        <a key="IQ_BETA_5YR">this</a>
        <b>beta5Yr</b>
      </field>
      <field>
        <a key="IQ_BV_SHARE">this</a>
        <b>bvShare</b>
      </field>
      <field>
        <a key="IQ_EBITDA_MARGIN">this</a>
        <b>ebitdaMargin</b>
      </field>
      <field>
        <a key="IQ_NI_MARGIN">this</a>
        <b>netProfitMargin</b>
      </field>
      <field>
        <a key="IQ_DILUT_EPS_NORM">this</a>
        <b>eps</b>
      </field>
      <field>
        <a key="IQ_TOTAL_DEBT_EQUITY">this</a>
        <b>totalDebtEquity</b>
      </field>
      <field>
        <a key="IQ_TOTAL_REV_1YR_ANN_GROWTH">this</a>
        <b>totalRev1YrAnnGrowth</b>
      </field>
      <field>
        <a key="IQ_TOTAL_REV_3YR_ANN_GROWTH">this</a>
        <b>totalRev3YrAnnGrowth</b>
      </field>
      <field>
        <a key="IQ_TOTAL_REV_5YR_ANN_GROWTH">this</a>
        <b>totalRev5YrAnnGrowth</b>
      </field>
      <field>
        <a key="IQ_AVG_BROKER_REC_NO_CIQ">this</a>
        <b>avgBrokerReq</b>
      </field>
      <field>
        <a key="IQ_BV_SHARE">this</a>
        <b>bvShare</b>
      </field>
  </mapping> 
            
</mappings>  
        
				},

				"eps": 
				{
					"type": "double"
				},

				"totalDebtEquity": 
				{
					"type": "double"
				},

				"totalRev1YrAnnGrowth": 
				{
					"type": "double"
				},

				"totalRev3YrAnnGrowth": 
				{
					"type": "double"
				},

				"totalRev5YrAnnGrowth": 
				{
					"type": "double"
				},

				"priceToBookRatio": 
				{
					"type": "double"
				}
			}
		},

		"financial": 
		{
			"dynamic": false,
			"properties": 
			{
				"tickerCode": 
				{
					"type": "string",
					"index": "not_analyzed"

{
	"settings": 
	{
		"number_of_shards": 5,
		"analysis": 
		{
			"analyzer": 
			{
				"mstar_text_ngram_analyzer": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"mstar_text_ngram_tokenizer"
					],

					"type": "custom",
					"tokenizer": "standard"
				}
			},

			"filter": 
			{
				"mstar_text_ngram_tokenizer": 
				{
					"type": "edgeNGram",
					"min_gram": "2",
					"max_gram": "20"
				}
			}
		}
	},

	"mappings": 
	{
		"company": 
		{
			"dynamic": false,
			"_id": 
			{
				"path": "tickerCode"
			},

			"properties": 
			{
				"tickerCode": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"gvKey": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"marketCap": 
				{
					"type": "double"
				},

				"volume": 
				{
					"type": "double"
				},

				"dividendYield": 
				{
					"type": "double"
				},

				"peRatio": 
				{
					"type": "double"
				},

				"totalRevenue": 
				{
					"type": "double"
				},

				"industry": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"industryGroup": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"priceVs52WeekHigh": 
				{
					"type": "double"
				},

				"priceVs52WeekLow": 
				{
					"type": "double"
				},

				"ebitdaMargin": 
				{
					"type": "double"
				},

				"netProfitMargin": 
				{
					"type": "double"
				},

				"beta5Yr": 
				{

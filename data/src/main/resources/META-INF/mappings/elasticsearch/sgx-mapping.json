{
	"settings": 
	{
		"number_of_shards": 5,
		"analysis": 
		{
			"analyzer": 
			{
				"full_name": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"partial_name": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"sgx_edgeNgram_tokenizer"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"partial_middle": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"sgx_ngram_tokenizer"
					],

					"type": "custom",
					"tokenizer": "standard"
				},

				"partial_back": 
				{
					"filter": 
					[
						"standard",
						"lowercase",
						"asciifolding",
						"sgx_edgeNgram_tokenizer_back"						
					],

					"type": "custom",
					"tokenizer": "standard"
				}
			},

			"filter": 
			{
				"sgx_edgeNgram_tokenizer": 
				{
				"side":"front",
					"type": "edgeNgram",
					"min_gram": "2",
					"max_gram": "20"
				},

				"sgx_edgeNgram_tokenizer_back": 
				{
				"side":"back",
					"type": "edgeNgram",
					"min_gram": "2",
					"max_gram": "20"
				},

				"sgx_ngram_tokenizer": 
				{
					"type": "ngram",
					"min_gram": "2",
					"max_gram": "20"
				}
			}
		}
	},

	"mappings": 
	{
		"company": 
		{
			"dynamic": false,
			"_id": 
			{
				"path": "tickerCode"
			},

			"properties": 
			{
				"tickerCode": 
				{
					"type": "multi_field",
					"path": "just_name",
					"fields": 
					{
						"tickerCode": 
						{
							"type": "string",
							"index": "not_analyzed"
						},

						"tickerSearch": 
						{
							"type": "multi_field",
							"fields": 
							{
								"tickerSearch": 
								{
									"type": "string",
									"analyzer": "full_name"
								},

								"partial": 
								{
									"type": "string",
									"search_analyzer": "full_name",
									"index_analyzer": "partial_name"
								},

								"partial_middle": 
								{
									"type": "string",
									"search_analyzer": "full_name",
									"index_analyzer": "partial_middle"
								},

								"partial_back": 
								{
									"type": "string",
									"search_analyzer": "full_name",
									"index_analyzer": "partial_back"
								}
							}
						}
					}
				},

				"avgBrokerReq": 
				{
					"type": "double"
				},

				"beta5Yr": 
				{
					"type": "double"
				},

				"companyName": 
				{
					"type": "multi_field",
					"fields": 
					{
						"companyName": 
						{
							"type": "string",
							"analyzer": "full_name"
						},

						"partial": 
						{
							"type": "string",
							"search_analyzer": "full_name",
							"index_analyzer": "partial_name"
						},

						"partial_middle": 
						{
							"type": "string",
							"search_analyzer": "full_name",
							"index_analyzer": "partial_middle"
						},

						"partial_back": 
						{
							"type": "string",
							"search_analyzer": "full_name",
							"index_analyzer": "partial_back"
						}
					}
				},

				"dividendYield": 
				{
					"type": "double"
				},

				"ebitdaMargin": 
				{
					"type": "double"
				},

				"eps": 
				{
					"type": "double"
				},

				"gvKey": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"industry": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"industryGroup": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"marketCap": 
				{
					"type": "double"
				},

				"netProfitMargin": 
				{
					"type": "double"
				},

				"peRatio": 
				{
					"type": "double"
				},

				"priceToBookRatio": 
				{
					"type": "double"
				},

				"priceVs52WeekHigh": 
				{
					"type": "double"
				},

				"priceVs52WeekLow": 
				{
					"type": "double"
				},

				"priceHistory": 
				{
					"type": "nested",
					"properties": 
					{
						"value": 
						{
							"type": "double"
						},

						"date": 
						{
							"type": "date"
						}
					}
				},

				"targetPriceNum": 
				{
					"type": "double"
				},

				"totalDebtEquity": 
				{
					"type": "double"
				},

				"totalRev1YrAnnGrowth": 
				{
					"type": "double"
				},

				"totalRev3YrAnnGrowth": 
				{
					"type": "double"
				},

				"totalRev5YrAnnGrowth": 
				{
					"type": "double"
				},

				"totalRevenue": 
				{
					"type": "double"
				},

				"volume": 
				{
					"type": "double"
				}
			}
		},

		"financial": 
		{
			"dynamic": false,
			"properties": 
			{
				"tickerCode": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"absPeriod": 
				{
					"type": "string",
					"index": "not_analyzed"
				}
			}
		},

		"price": 
		{
			"dynamic": false,
			"properties": 
			{
				"tickerCode": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"date": 
				{
					"type": "date"
				},

				"value": 
				{
					"type": "double"
				}
			}
		},

		"volume": 
		{
			"dynamic": false,
			"properties": 
			{
				"tickerCode": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"date": 
				{
					"type": "date"
				},

				"value": 
				{
					"type": "double"
				}
			}
		},

		"holders": 
		{
			"dynamic": false,
			"properties": 
			{
				"tickerCode": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"holder": 
				{
					"type": "object",
					"properties": 
					{
						"name": 
						{
							"type": "string",
							"index": "not_analyzed"
						},

						"shares": 
						{
							"type": "long"
						},

						"percent": 
						{
							"type": "double"
						}
					}
				}
			}
		},

		"keyDevs": 
		{
			"dynamic": false,
			"properties": 
			{
				"tickerCode": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"keyDev": 
				{
					"type": "object",
					"properties": 
					{
						"headline": 
						{
							"type": "string",
							"index": "not_analyzed"
						},

						"date": 
						{
							"type": "date"
						},

						"situation": 
						{
							"type": "string",
							"index": "not_analyzed"
						}
					}
				}
			}
		},

		"alphaFactor": 
		{
			"dynamic": false,
			"_id": 
			{
				"path": "id"
			},

			"properties": 
			{
				"id": 
				{
					"type": "string",
					"index": "not_analyzed"
				},

				"date": 
				{
					"type": "date"
				},

				"analystExpectations": 
				{
					"type": "integer"
				},

				"capitalEfficiency": 
				{
					"type": "integer"
				},

				"earningsQuality": 
				{
					"type": "integer"
				},

				"historicalGrowth": 
				{
					"type": "integer"
				},

				"priceMomentum": 
				{
					"type": "integer"
				},

				"valuation": 
				{
					"type": "integer"
				},

				"volatility": 
				{
					"type": "integer"
				},

				"size": 
				{
					"type": "integer"
				}
			}
		},

		"_default_": 
		{
			"dynamic": false,
			"properties": 
			{
				"id": 
				{
					"type": "string",
					"index": "not_analyzed",
					"index_name": "id"
				}
			}
		}
	}
}